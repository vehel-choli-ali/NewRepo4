using System;

namespace ConsoleApp3
{
    internal class Program
    {
        static void Main()
        {
            int[] numbers = { 1, 7, 5, 6, 9, 3, 12 };

            Console.WriteLine("Numbers greater than 5:");
            foreach (var n in numbers)
            {
                if (n > 5)
                {
                    Console.WriteLine(n);
                }
            }
        }
    }
}
